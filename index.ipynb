    "# Sample Python code\n",
    "\n",
    "# Function to greet a list of names\n",
    "def greet_names(names):\n",
    "    for name in names:\n",
    "        print(f\"Hello, {name}!\")\n",
    "\n",
    "# List of names\n",
    "names_list = [\"Alice\", \"Bob\", \"Charlie\", \"Diana\"]\n",
    "\n",
    "# Call the function with the list of names\n",
    "greet_names(names_list)\n"
